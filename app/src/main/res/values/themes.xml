<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="My_theme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <item name="colorSecondary">@color/colorAccent</item>
        <item name="colorSecondaryVariant">#0039CB</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <item name="colorError">#F44336</item>
        <item name="colorOnError">#FFFFFF</item>
        <item name="colorSurface">#FFFFFF</item>
        <item name="colorOnSurface">#212121</item>
        <item name="android:colorBackground">@color/chip_color</item>
        <item name="colorOnBackground">#212121</item>
    </style>

    <style name="My_theme_v2" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary_v2</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <item name="colorSecondary">@color/colorAccent</item>
        <item name="colorSecondaryVariant">#00C853</item>
        <item name="colorOnSecondary">#FFFFFF</item>
        <item name="colorError">#F44336</item>
        <item name="colorOnError">#FFFFFF</item>
        <item name="colorSurface">@color/chip_color_night</item>
        <item name="colorOnSurface">#D4E3DA</item>
        <item name="android:colorBackground">@color/colorPrimary</item>
        <item name="colorOnBackground">#212121</item>

    </style>

    <style name="Button_style" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="backgroundTint">@color/purple_500</item>
    </style>



</resources>